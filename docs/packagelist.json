{
	"repo":"Pluto Repo",
	"packages": [
	    {
			"name":"cowsay",
			"git":"https://github.com/cowsay-org/cowsay.git",
			"description":"Cowsay is a configurable talking cow, written in Perl.",
			"authors": [
			    "Andrew Janke",
				"Tony Monroe"
			],
			"needs": [
			    "make"
			],
			"build":[
			    "sudo make install"
			],
			"remove":[
			    "sudo make uninstall"
			]
		},
		{
			"name":"pluto",
			"git":"https://github.com/Turtley12/pluto.git",
			"description":"Almost a package manager. kind of pointless to have it in it's own package list.",
			"authors": [
				"Turtley12"
			],
			"needs": [
			    "go"
			],
			"build":[
			    "bash scripts/install.sh"
			],
			"remove":[
			    "bash scripts/uninstall.sh"
			]
		}
	]
}
